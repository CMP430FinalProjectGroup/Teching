<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.team2.teching">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TechingApp">

        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.project.team2.teching.activities.NewUsersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.MainActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.NewUsersInfoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.NewUsersActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.TermsOfServiceActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.NewUsersInfoActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.FirstPageProfileInfoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.TermsOfServiceActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.SecondPageProfileInfoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.FirstPageProfileInfoActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.UserBioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.MainActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.FindPartner">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.UserBioActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.ChatActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.UserBioActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.MessagesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.FindPartner" />
        </activity>

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".activities.ChatActivity" />

        <activity android:name="com.project.team2.teching.activities.UserProfile">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.UserBioActivity" />
        </activity>

        <activity android:name="com.project.team2.teching.activities.ExistingUsersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.MainActivity" />
        </activity>

    </application>

</manifest>